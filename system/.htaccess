<IfModule authz_core_module>
	Require all denied
</IfModule>
<IfModule !authz_core_module>
	Deny from all
</IfModule>
<IfModule mod_rewrite.c>
	RewriteEngine On
	RewriteBase /pinterestelar
	RewriteCond %{REQUEST_FILENAME} !-f
	RewriteCond %{REQUEST_FILENAME} !-d
	RewriteRule ^(.*)$ index.php/$1 [L]
</IfModule>
<IfModule !mod_rewrite.c>
	ErrorDocument 404 /pinterestelar/index.php
</IfModule>